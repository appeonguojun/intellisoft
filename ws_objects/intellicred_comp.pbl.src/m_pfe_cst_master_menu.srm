event clicked;//--------------------------- APPEON BEGIN ---------------------------
//$<modify> 2005-12-05 By: Luke
//$<reason> Update the connection parm for SQL database and add check
//$<reason> code.

//COMMIT USING SQLCA;
//
//DISCONNECT USING SQLCA;
//
//SQLCA.DBMS="ODBC"											//uid and pwd hardcoded to master to stop crash maha 102500
//SQLCA.DbParm="ConnectString='DSN=CentralServices;UID=master;PWD=intellimaster;,DelimitIdentifier='No'"
//SQLCA.AutoCommit = FALSE
//
//CONNECT USING SQLCA;

transaction ltr_temp //keep local trasaction for restore

COMMIT USING SQLCA;

ltr_temp = create transaction 
ltr_temp.dbms = SQLCA.DBMS
ltr_temp.database = SQLCA.Database
ltr_temp.servername = SQLCA.ServerName
ltr_temp.logid = SQLCA.LogId
ltr_temp.logpass = SQLCA.LogPass 
ltr_temp.autocommit = SQLCA.AutoCommit
ltr_temp.Dbparm = SQLCA.DBParm

DISCONNECT USING SQLCA;

choose case gs_dbtype 
  case 'ASA' 
	SQLCA.DBMS="ODBC"											//uid and pwd hardcoded to master to stop crash maha 102500
	SQLCA.DbParm="ConnectString='DSN=CentralServices;UID=dba;PWD=sql;,DelimitIdentifier='Yes'"
	SQLCA.AutoCommit = FALSE
  case "SQL"
	SQLCA.DBMS="ODBC"											//uid and pwd hardcoded to master to stop crash maha 102500
	SQLCA.DbParm="ConnectString='DSN=CentralServices;UID=sa;PWD=;,DelimitIdentifier='Yes'"
	SQLCA.AutoCommit = FALSE
end choose

//--------------------------- APPEON BEGIN ---------------------------
//$< Add  > 2007-09-14 By: Scofield
//$<Reason> Set the connection cache dynamically on Web
if AppeonGetClientType() = 'WEB' then
	SQLCA.DBMS = gs_CacheType
	SQLCA.DBParm = gs_CacheSet
end if
//---------------------------- APPEON END ----------------------------

CONNECT USING SQLCA;
IF SQLCA.SQLCODE = -1 THEN
	MessageBox( "Error", SQLCA.SQLERRTEXT )
	
	//restore connection
	SQLCA.dbms = ltr_temp.DBMS
	SQLCA.database = ltr_temp.Database
	SQLCA.servername = ltr_temp.ServerName
	SQLCA.logid = ltr_temp.LogId
	SQLCA.logpass = ltr_temp.LogPass 
	SQLCA.autocommit = ltr_temp.AutoCommit
	SQLCA.Dbparm = ltr_temp.DBParm
	
	//--------------------------- APPEON BEGIN ---------------------------
	//$< Add  > 2007-09-14 By: Scofield
	//$<Reason> Set the connection cache dynamically on Web
	if AppeonGetClientType() = 'WEB' then
		SQLCA.DBMS = gs_CacheType
		SQLCA.DBParm = gs_CacheSet
	end if
	//---------------------------- APPEON END ----------------------------

	CONNECT USING SQLCA;
	
	Return 
END IF

//---------------------------- APPEON END ----------------------------


