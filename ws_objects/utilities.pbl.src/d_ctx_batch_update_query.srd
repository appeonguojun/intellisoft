 column=(type=char(30) updatewhereclause=yes name=department_name dbname="department_name" )
 column=(type=char(50) updatewhereclause=yes name=ctx_templates_info_template_name dbname="ctx_templates_info.template_name" )
 retrieve="
Select 
ctx_basic_info.ctx_id,
company = app_facility.facility_name,
category_name = a.code,
division_name = b.code,
department_name = c.code,
ctx_templates_info.template_name
From ctx_basic_info 
Left Outer Join app_facility on (ctx_basic_info.app_facility = app_facility.facility_id  )
Left Outer Join code_lookup a on (ctx_basic_info.category =  a.lookup_code)
Left Outer Join code_lookup b on (ctx_basic_info.dvision =  b.lookup_code)
Left Outer Join code_lookup c on (ctx_basic_info.custom_n15 =  c.lookup_code)
Left Outer Join ctx_templates_info on (ctx_basic_info.ctx_id = ctx_templates_info.ctx_id )
Where 

ctx_basic_info.category = :al_category and ctx_basic_info.dvision = :al_division and ctx_basic_info.custom_n15 = :al_department
 and (ctx_basic_info.ctx_id > 0 or 1 = :ai_includetemplate)
