 retrieve="  SELECT contracted_entity = ( select top 1 group_multi_loc.gp_name
                               from ctx_loc,group_multi_loc
                               where (ctx_loc.parent_comp_id = group_multi_loc.rec_id)
                               and (ctx_loc.ctx_id = ctx_basic_info.ctx_id)
                               and ctx_loc.loc_id = 0 ),
         productname = code_lookup_c.code,   
         ctx_basic_info.expriation_date,   
         ctx_basic_info.review_date,   
         c_fullname = ctx_contacts.first_name + ',' + ctx_contacts.last_name + CASE isnull( ltrim( rtrim(upper(ctx_contacts.middle_name)) ), '' ) 								WHEN '' THEN '' WHEN 'NONE' THEN '' 								ELSE ',' + ctx_contacts.middle_name 							 END,   
         app_facility.facility_name,   
         ctx_status = code_lookup_a.code,   
         ctx_type = code_lookup_b.code,   
			ctx_basic_info.ctx_id  
    FROM {oj ctx_contacts RIGHT OUTER JOIN ctx_contract_contacts ON ctx_contacts.contact_id = ctx_contract_contacts.contact_id}, {oj ctx_basic_info LEFT OUTER JOIN app_facility ON ctx_basic_info.app_facility = app_facility.facility_id LEFT OUTER JOIN code_lookup code_lookup_a ON ctx_basic_info.status = code_lookup_a.lookup_code LEFT OUTER JOIN code_lookup code_lookup_b ON ctx_basic_info.contract_type = code_lookup_b.lookup_code LEFT OUTER JOIN ctx_products ON ctx_basic_info.ctx_id = ctx_products.ctx_id LEFT OUTER JOIN code_lookup code_lookup_c ON ctx_products.product_id = code_lookup_c.lookup_code}  
   WHERE ( ctx_contract_contacts.ctx_id = ctx_basic_info.ctx_id ) and  
			( ctx_basic_info.is_template = 0) and
         ( ( ctx_contract_contacts.owner = 'Y' ) AND  
         ( ctx_contract_contacts.ic_area = 'I' ) )    
"  sort="ctx_type A app_facility_facility_name A ctx_basic_info_ctx_id A " )
