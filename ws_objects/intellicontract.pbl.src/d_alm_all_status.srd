 retrieve="  SELECT ctx_all_notification.ctx_id,   
         ctx_all_notification.doc_id,   
         ctx_all_notification.level_id1,   
         ctx_all_notification.level_id2,   
         ctx_all_notification.alm_table,   
         ctx_all_notification.alm_column,   
         ctx_all_notification.alm_message,   
         ctx_all_notification.alm_enabled,   
         ctx_all_notification.alm_notification_days,   
         ctx_all_notification.alm_last_notification_sent,   
         ctx_all_notification.alm_post_notification_days,   
         ctx_all_notification.alm_total_tries,   
         ctx_all_notification.alm_action_item_posted,   
         ctx_all_notification.alm_total_tries_counter,   
         ctx_all_notification.alm_frequency,   
         ctx_all_notification.create_action_item,   
         ctx_all_notification.record_id,   
         ctx_all_notification.alm_auto_increment_years,   
         ctx_all_notification.alm_action_title,   
         ctx_all_notification.wf_id,   
         ctx_all_notification.alm_auto_increment_unit,   
         ctx_all_notification.alm_last_wf_trigger,   
         ctx_all_notification.alm_email_template_id,   
         ctx_all_notification.alm_attachment_name,   
		alm_mailto = Replace(ctx_all_notification.alm_mailto ,'$<OwnerEmail>$', isnull( security_users.email_id ,'')    ) ,  
         ctx_all_notification.auto_sign,   
         alm_create = Replace(ctx_all_notification.alm_create, '$<Owner>$', Isnull(ctx_contacts.user_d,'')),  
         ctx_all_notification.alm_subject,   
         ctx_all_notification.alm_date,   
         curdate = getdate(),   
         ctx_basic_info.status,   
		app_facility.facility_name,
         alm_message_length = datalength(alm_message_blob),   
         alm_attachment_length = datalength(alm_attachment),   
         email_notification = 0,   
         wf_trigger = 0,   
         ctx_basic_info.category,   
         ctx_contacts.user_d  , 
		ctx_all_notification.alm_appointment,
		security_users.email_id
    FROM ctx_all_notification,   
         ctx_basic_info  LEFT OUTER JOIN app_facility ON ctx_basic_info.app_facility = app_facility.facility_id ,
         code_lookup,   
         ctx_contract_contacts,   
         ctx_contacts  Left Outer Join security_users On (ctx_contacts.user_d = security_users.user_id )
   WHERE ( ctx_basic_info.ctx_id = ctx_all_notification.ctx_id ) and  
         ( ctx_basic_info.status = code_lookup.lookup_code ) and  
         ( ctx_contract_contacts.ctx_id = ctx_basic_info.ctx_id ) and  
         ( ctx_contacts.contact_id = ctx_contract_contacts.contact_id ) and  
         ( ( code_lookup.lookup_name = 'Contract Status' ) AND  
         ( code_lookup.custom_1 = 'YES' ) AND  
         ( ctx_all_notification.alm_enabled = '1' ) AND  
         ( ctx_basic_info.is_template = 0 ) AND  
         (ctx_all_notification.alm_total_tries_counter is null OR  
         ctx_all_notification.alm_total_tries <> ctx_all_notification.alm_total_tries_counter) AND  
         ctx_contract_contacts.owner = 'Y' AND  
         ctx_contract_contacts.ic_area = 'I' )   
ORDER BY ctx_basic_info.ctx_id ASC   

" update="ctx_all_notification" updatewhere=0 updatekeyinplace=no )
