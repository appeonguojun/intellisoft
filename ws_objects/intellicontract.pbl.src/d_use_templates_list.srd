 retrieve="  SELECT ctx_templates_info.id,   
         ctx_templates_info.template_name,   
         ctx_templates_info.create_date,   
         ctx_templates_info.create_by,   
         ctx_templates_info.ctx_id,   
         ctx_templates_info.original_ctx_id,   
         ctx_templates_info.ib_contract_detail,   
         ctx_templates_info.ib_contract_dates,   
         ctx_templates_info.ib_custom_data,   
         ctx_templates_info.ib_notes,   
         ctx_templates_info.ib_contract_contacts,   
         ctx_templates_info.ib_group_access,   
         ctx_templates_info.ib_products,   
         ctx_templates_info.ib_our_contacts,   
         ctx_templates_info.ib_requirements,   
         ctx_templates_info.ib_other_contacts,   
         ctx_templates_info.ib_locations,   
         ctx_templates_info.ib_organizations,   
         ctx_templates_info.ib_fee_schedule,   
         ctx_templates_info.ib_document_manager,   
         ctx_templates_info.ib_action_items,   
         ctx_templates_info.notes,   
         ctx_templates_info.located_tab_index,   
         ctx_templates_info.clear_company,   
         ctx_templates_info.add_contact,   
         ctx_templates_info.trigger_workflow,   
         ctx_templates_info.ib_owner,   
         ctx_templates_info.ib_default_email,   
         ctx_templates_info.ib_custom_data2,   
         ctx_templates_info.ib_custom_multi  
    FROM ctx_templates_info,   
         ctx_basic_info  
   WHERE ( ctx_templates_info.ctx_id = ctx_basic_info.ctx_id )    
   And ctx_basic_info.ctx_id IN 
(
	select ctx_id from ( 

		SELECT DISTINCT TOP 100 Percent  ctx_id = ctx_contract_contacts.ctx_id, user_id = ctx_contacts.user_d, access = (CASE ctx_contract_contacts.owner WHEN 'Y' THEN 'F' ELSE ctx_contract_contacts.access END)
				 FROM ctx_contract_contacts Left Outer Join
				 (
				 Select ctx_contract_contacts1.ctx_id,ctx_contacts1.user_d 
				 From ctx_contract_contacts ctx_contract_contacts1 ,ctx_contacts ctx_contacts1
				 Where ctx_contract_contacts1.contact_id = ctx_contacts1.contact_id And ctx_contacts1.user_d = :USER_ID
				 Group By ctx_contract_contacts1.ctx_id,ctx_contacts1.user_d
				 Having Count(ctx_contract_contacts1.ctx_id) > 1
				 ) ccc
				 On ctx_contract_contacts.ctx_id = ccc.ctx_id
				 ,ctx_contacts, ctx_basic_info
				 WHERE ctx_contacts.user_d = :USER_ID and ctx_contract_contacts.ctx_id = ctx_basic_info.ctx_id
					AND ctx_basic_info.is_template = 1
						  AND (ccc.ctx_id is null or ctx_contract_contacts.access = 'F' )
						  AND ctx_contract_contacts.contact_id = ctx_contacts.contact_id 
						  AND ( ctx_contract_contacts.access IN ('F','R') 
						  OR ctx_contract_contacts.owner = 'Y' )
						  AND ctx_contract_contacts.ic_area = 'I'
				 
				 UNION 
				 
		SELECT DISTINCT TOP 100 Percent ctx_id = ctx_group_access.ctx_id,user_id = security_group_users.user_id, access = (CASE ctx_group_access.access WHEN 'R' THEN 'F' ELSE ctx_group_access.access END)
		FROM 
		ctx_group_access ,
		security_group_users,
		icred_settings,ctx_basic_info
		WHERE  ctx_group_access.ctx_id = ctx_basic_info.ctx_id AND
		ctx_basic_info.is_template = 1 AND
		security_group_users.user_id = :USER_ID
				 AND ctx_group_access.group_id = security_group_users.group_id
				 AND ctx_group_access.access = 
						  (Case When Exists (Select u1.user_id,a1.ctx_id,a1.access 
						  From ctx_group_access a1,security_group_users u1
						  Where a1.group_id = u1.group_id 
						  And a1.ctx_id = ctx_group_access.ctx_id
						  And u1.user_id = security_group_users.user_id   
						  And a1.access = 'F') Then 'F' Else 'R' End) 
				 AND icred_settings.group_access = 1
				 AND ctx_group_access.access in ('F','R')
				 AND NOT EXISTS       (SELECT 1 FROM ctx_contract_contacts, ctx_contacts  
									WHERE ( ctx_contacts.contact_id = ctx_contract_contacts.contact_id ) 
									AND ( ctx_contacts.user_d = security_group_users.user_id )
									AND ( ctx_contract_contacts.ic_area = 'I' )
									AND ( ctx_contract_contacts.ctx_id = ctx_group_access.ctx_id )
									)
				 
				 UNION
				 
		SELECT b.ctx_id,b.user_id,(CASE WHEN b.rcnt=0 THEN 'F' ELSE 'R' END) access 
		FROM
		(
				 SELECT DISTINCT a.ctx_id,a.user_id,
						  SUM(CASE a.access WHEN 'F' THEN 1 ELSE 0 END) fcnt,
						  SUM(CASE a.access WHEN 'R' THEN 1 ELSE 0 END) rcnt
				 FROM 
				 (
						  SELECT DISTINCT TOP 100 Percent ctx_id = ctx_basic_info.ctx_id, 
						  user_id = security_users.user_id,
						  
						  access = (CASE WHEN ctx_acp_contract_management_right.rights IN('11','61','71','81') THEN 'F' 
														 WHEN ctx_acp_contract_management_right.rights IN('12','62','72','82') THEN 'R' 
												 ELSE 'N' END) 
						  FROM ctx_acp_contract_management_right, security_users, ctx_basic_info 
						  WHERE security_users.user_id = :USER_ID   
						  AND ctx_acp_contract_management_right.role_id = security_users.role_id 
						  And ctx_basic_info.is_template = 1  
						  AND ((ctx_acp_contract_management_right.types=1
						  AND ctx_acp_contract_management_right.category = ctx_basic_info.category 
						  AND ctx_acp_contract_management_right.rights In ('11','12','13')) OR 
						  (ctx_acp_contract_management_right.types=6 
						  AND ctx_acp_contract_management_right.category = ctx_basic_info.dvision  
						  AND ctx_acp_contract_management_right.rights In ('61','62','63')) OR  
						  (ctx_acp_contract_management_right.types=7 
						  AND ctx_acp_contract_management_right.category = ctx_basic_info.contract_department 
						  AND ctx_acp_contract_management_right.rights In ('71','72','73')) OR 
						  (ctx_acp_contract_management_right.types=8 
						  AND ctx_acp_contract_management_right.category = ctx_basic_info.contract_type 
						  AND ctx_acp_contract_management_right.rights In ('81','82','83'))) 
									AND NOT EXISTS (SELECT 1 FROM ctx_contract_contacts, ctx_contacts  
											WHERE ( ctx_contacts.contact_id = ctx_contract_contacts.contact_id ) 
											AND ( ctx_contacts.user_d = security_users.user_id AND ctx_contract_contacts.ic_area = 'I') 
											AND ( ctx_contract_contacts.ctx_id = ctx_basic_info.ctx_id ) 
											UNION
											SELECT 1 FROM ctx_group_access,security_group_users,icred_settings
											WHERE icred_settings.group_access = 1
											AND (ctx_group_access.group_id = security_group_users.group_id )
											AND ( security_group_users.user_id = security_users.user_id ) 
											AND ( ctx_group_access.ctx_id = ctx_basic_info.ctx_id )
											)
				 ) a 
				 GROUP BY a.ctx_id,a.user_id 
			HAVING SUM(CASE a.access WHEN 'N' THEN 1 ELSE 0 END) = 0 
		) b
		
		)bb where bb.access = 'F'

)
" arguments=(("USER_ID", string))  sort="template_name A " )
