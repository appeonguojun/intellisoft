		right2 = (case WHEN security_rights_action.dddw_type=2 AND security_roles_rights.value_id=1 then 1 else 0 end),
		right3 = (case WHEN security_roles_rights.value_id=0 then 1 else 0 end)  
    FROM security_rights_action,   
         security_roles_rights  
   WHERE ( security_roles_rights.action_id = security_rights_action.action_id ) and  
         ( ( security_rights_action.sub_module_id = :sub_module_id ) AND  
         ( security_roles_rights.role_id = :role_id ) )    
