public subroutine of_encrypt_esign_pwd ();//====================================================================
// Function: of_encrypt_esign_pwd
//--------------------------------------------------------------------
// Description:Encrypt Esign License pwd
//--------------------------------------------------------------------
// Arguments:
//--------------------------------------------------------------------
// Returns:  (None)
//--------------------------------------------------------------------
// Author:         Ken.Guo 2017-08-18
//--------------------------------------------------------------------
//Copyright (c) 2008-2016 Contract Logix, Inc. All rights reserved.
//--------------------------------------------------------------------
// Modify History:
//
//====================================================================


String ls_pwd, ls_pwd_new
long i
n_cst_encrypt lnv_Encrypt
n_ds lds_personal_license

//Encrypt Esign Global License pwd
Select docusign_password into :ls_pwd from  icred_settings where encrypt_esign_pwd is null or encrypt_esign_pwd <> 1;
If len(ls_pwd) > 0 Then
	ls_pwd_new = lnv_Encrypt.of_encrypt( ls_pwd )
	Update icred_settings set docusign_password = :ls_pwd_new, encrypt_esign_pwd = 1 ;
	If sqlca.sqlcode <> 0 Then
		gnv_debug.of_output(True, 'Failed to encrypt the E-sign global license password.')
	End If
End If

//Encrypt Esign Personal License pwd
lds_personal_license = Create n_ds
lds_personal_license.dataobject = 'd_security_license_user_all'
lds_personal_license.Settransobject(sqlca)
lds_personal_license.retrieve()
For i = 1 To lds_personal_license.rowcount()
	ls_pwd = lds_personal_license.GetitemString(i, 'esign_password')
	ls_pwd_new = lnv_Encrypt.of_encrypt( ls_pwd )
	lds_personal_license.setitem(i, 'esign_password', ls_pwd_new)
Next
If  lds_personal_license.ModifiedCount() > 0 Then
	If lds_personal_license.Update()  <> 1 Then
		gnv_debug.of_output(True, 'Failed to encrypt the E-sign personal license password.')
	End If
End If
Destroy lds_personal_license


end subroutine

