SELECT set_45 INTO :ll_EnKeyVal FROM icred_settings;

if ll_EnKeyVal < 126 or ll_EnKeyVal > 160 then
	lds_UserPass = Create DataStore
	lds_UserPass.DataObject = 'd_user_password'
	lds_UserPass.SetTransObject(SQLCA)
	lds_UserPass.Retrieve()
	
	ll_Count = lds_UserPass.RowCount()
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_UserPass.GetItemString(ll_Cycle,'user_password')
		ls_Password = lnvo_Encrypt.of_Decrypt(ls_Password)
		lds_UserPass.SetItem(ll_Cycle,'user_password',ls_Password)
	next
	
	UPDATE icred_settings SET set_45 = 144;
	
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_UserPass.GetItemString(ll_Cycle,'user_password')
		ls_Password = lnvo_Encrypt.of_Encrypt(ls_Password)
		lds_UserPass.SetItem(ll_Cycle,'user_password',ls_Password)
	next
	
	if ll_Count > 0 then
		if lds_UserPass.Update() <> 1 then
			ROLLBACK USING SQLCA;
			
			ls_Error += "n_cst_update_data.of_ver85_release_data3() Information: The Encryption Value has been automatically changed from " + String(ll_EnKeyVal) + " to 144 during the upgrading process, if there are users that cannot login the system, please call Support."
			if IsValid(gnv_logservice) then
				gnv_logservice.of_setloglevel(2) //warning level
				gnv_logservice.of_log_warning(ls_Error)
				gb_upgrade_failed = True
			end if
		else
			COMMIT USING SQLCA;
		end if
	end if
	
	Destroy lds_UserPass
end if

if ll_ColLen < 100 then
	lds_EmailPass = Create DataStore
	lds_EmailPass.DataObject = 'd_email_password'
	lds_EmailPass.SetTransObject(SQLCA)
	lds_EmailPass.Retrieve()
		
	ll_Count = lds_EmailPass.RowCount()
	for ll_Cycle = 1 to ll_Count
		ls_Password = lds_EmailPass.GetItemString(ll_Cycle,'password')
		if IsNull(ls_Password) then CONTINUE
		
		ls_Password = lnvo_Encrypt.of_Encrypt(ls_Password)
		lds_EmailPass.SetItem(ll_Cycle,'password',ls_Password)
	next
	
	if ll_Count > 0 then
		if lds_EmailPass.Update() <> 1 then
			ROLLBACK USING SQLCA;
			
