public function integer of_ver140_updates ();//Added By Mark Lee 03/03/2014
of_ver140_release_data1()

//Added By Mark 06/26/2014
String ls_main_version,ls_patch_version
Select set_18,set_patch_ver Into :ls_main_version, :ls_patch_version From icred_settings;
If  ( ls_main_version = '140' and ls_patch_version < '027' ) Then
	Update request_form_detail	
	Set formats = export_fields.export_format
	From request_form_detail, export_fields
	Where request_form_detail.rec_id = export_fields.rec_id  ;
End if
		
//Added By Mark 07/10/2014
If  ls_main_version < '140' or ( ls_main_version = '140' and ls_patch_version < '033' ) Then
//	Update export_fields 
//	Set lu_field_name = 'description'
//	where ctx_scrn_id = 3 and ctx_column_name='term_reason'
//		and ctx_scrn_dataobject='d_contract_det_master'
//		 and ctx_lookup_type = 'C' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) ='' ) ;

	gnv_appeondb.of_startqueue( )
	Update export_fields 
		Set lu_field_name = 'code'  
	where ctx_table_name = 'ctx_basic_info'
	 and ctx_lookup_type = 'C' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) ='' ) ;
	 
	Update export_fields 
	Set lu_field_name = 'code'  
	where ctx_table_name = 'ctx_custom_multi_hdr'
	 and ctx_lookup_type = 'C' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) ='' ) ;

	 Update export_fields 
	Set lu_field_name = 'code'  
	where ctx_table_name = 'ctx_custom_multi_detail'
	 and ctx_lookup_type = 'C' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) ='' ) ;
 
	update  export_fields
	set lu_field_name ='first_name'
	where lower(export_field_name) = LOWER('Owner_first_name')
	and ctx_scrn_id =7 and ctx_column_name = 'contact_id' and ctx_lookup_type ='C'
	and ctx_table_name = 'ctx_contract_contacts' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) =''  ) ;

	update  export_fields
	set lu_field_name ='last_name'
	where lower(export_field_name) = LOWER('Owner_last_name')
	and ctx_scrn_id =7 and ctx_column_name = 'contact_id' and ctx_lookup_type ='C'
	and ctx_table_name = 'ctx_contract_contacts' and (lu_field_name is null or  ltrim(rtrim(lu_field_name)) ='' )  ;

	update  export_fields
	set lu_field_name ='contact_type'
	where lower(export_field_name) = LOWER('Owner_association')
	and ctx_scrn_id =7 and ctx_column_name = 'contact_id' and ctx_lookup_type ='C'
	and ctx_table_name = 'ctx_contract_contacts' and (lu_field_name is null or  ltrim(rtrim(lu_field_name)) =''  )  ;

	update  export_fields
	set lu_field_name ='phone'
	where lower(export_field_name) = LOWER('Owner_phone')
	and ctx_scrn_id =7 and ctx_column_name = 'contact_id' and ctx_lookup_type ='C'
	and ctx_table_name = 'ctx_contract_contacts' and (lu_field_name is null or ltrim(rtrim(lu_field_name)) =''  ) ;

	update  export_fields
	set lu_field_name ='contact_email'
	where lower(export_field_name) = LOWER('Owner_email')
	and ctx_scrn_id =7 and ctx_column_name = 'contact_id' and ctx_lookup_type ='C'
	and ctx_table_name = 'ctx_contract_contacts' and (lu_field_name is null or  ltrim(rtrim(lu_field_name)) =''  ) ;
	
	
	//update request form data
	update request_form_detail
	set column_width = 0
	where column_width is null ;
	
	update request_form_detail
	set column_height = (case when (export_fields.lookup_field_style is not null and export_fields.lookup_field_style <> '') 
				or (aa.col_type = 'datetime' or aa.col_type = 'smalldatetime' ) then 1 
				when (export_fields.lookup_field_style is null or export_fields.lookup_field_style = '') 
				and (aa.col_type <> 'datetime' and aa.col_type <> 'smalldatetime' ) and (aa.col_length is null or aa.col_length < 8000) then 2
				when (export_fields.lookup_field_style is null or export_fields.lookup_field_style = '') 
				and (aa.col_type <> 'datetime' and aa.col_type <> 'smalldatetime' ) and (aa.col_length is not null and aa.col_length = 8000) then 8
				else 2 end )
		FROM request_form_detail  left join export_fields on export_fields.export_id = request_form_detail.export_id 
			and export_fields.rec_id = request_form_detail.rec_id 
				left join (select object_name(syscolumns.id) as table_name, syscolumns.name as col_name, syscolumns.length as col_length,systypes.name as col_type, syscolumns.prec as prec, syscolumns.scale as scale
		from syscolumns , systypes where syscolumns.xtype = systypes.xtype ) aa on export_fields.ctx_table_name = aa.table_name and export_fields.ctx_column_name = aa.col_name
	where (column_height is null or column_height = 0 ) ;
	
	gnv_appeondb.of_commitqueue( )
End If

